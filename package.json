{
  "name": "sentari-transcript-empathy",
  "version": "1.0.0",
  "description": "Sentari Group Interview - Transcript to Empathy Pipeline - Team Vijayasimha",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/index.js",
    "dev": "ts-node src/index.ts",
    "simulate:first": "npm run build && node -e \"require('./dist/pipeline.js').simulateFirst()\"",
    "simulate:hundred": "npm run build && node -e \"require('./dist/pipeline.js').simulateHundred()\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "serve": "npm run build && node dist/server.js",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "test:unit": "jest tests/unit/",
    "test:performance": "jest tests/performance/",
    "test:load": "jest tests/load/",
    "qa-report": "node scripts/generate-qa-report.js",
    "test:integration": "jest tests/integration/",
    "test:e2e": "jest tests/e2e/",
    "test:all": "jest tests/"
  },
  "keywords": [
    "sentari",
    "empathy",
    "ai",
    "transcript",
    "vijayasimha"
  ],
  "author": "Team Vijayasimha - Associate Lead",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "bcrypt": "^5.1.1",
    "chart.js": "^4.0.0",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "recordrtc": "^5.6.2",
    "wavesurfer.js": "^7.0.0",
    "webrtc-adapter": "^8.2.3"
  }
}
